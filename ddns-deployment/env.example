# 阿里云 API 凭证
ALIBABA_CLOUD_ACCESS_KEY_ID=你的AccessKey_ID
ALIBABA_CLOUD_ACCESS_KEY_SECRET=你的AccessKey_Secret

# 域名配置
DOMAIN_NAME=your-domain.com          # 你的域名（不需要 http://）
SUB_DOMAIN=pm                         # 子域名前缀

# 可选：指定记录类型
RECORD_TYPE=A                          # 通常用 A 记录

# 可选：配置文件路径
CONFIG_DIR=~/.openclaw/ddns
LOG_FILE=~/ddns.log
